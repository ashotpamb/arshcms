{
    "name": "arshcms/base",
    "type": "project",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ashotpamb/arshcms.git"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Arshcms\\Models\\": " Models/",
